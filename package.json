    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^actions/(.*)": "<rootDir>/src/actions/$1",
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^data/(.*)": "<rootDir>/src/data/$1",
      "^editors/(.*)": "<rootDir>/src/editors/$1",
