      "^types/(.*)": "<rootDir>/src/types/$1",
      "^utils/(.*)": "<rootDir>/src/utils/$1"
    },
    "transform": {
      ".*": "<rootDir>/preprocessor.js"
    },
    "testRegex": "test.*-test.(ts|tsx|js)$",
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine-env.js"
