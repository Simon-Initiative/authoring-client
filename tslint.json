{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "variable-name": [
            true,
            "allow-pascal-case"
        ],
        "object-shorthand-properties-first": false,
        "no-unused-variable": true,
        "strict-boolean-expressions": false
    },
    "rulesDirectory": [],
    "tslint.autoFixOnSave": true,
}