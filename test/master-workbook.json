 // To use this file, open the couchdb web console and find a workbook page document

 // Then replace the top level 'body' attribute with the top level 'body'
 // attribute here:
 {
  "body": {
    "section": {
      "#array": [
        {
          "title": {
            "#text": "Section title"
          }
        },
        {
          "body": {
            "#array": [
              {
                "pullout": {
                  "@type": "note",
                  "p": {
                    "#text": " A simple paragraph in the note pullout. "
                  }
                }
              },
              {
                "p": {
                  "#array": [
                  {
                    "#text": "As we "
                  },
                  {
                    "link": {
                      "#text": "text hyperlink ",
                      "@target": "new",
                      "@href": "http://www.google.com"
                    }
                  },
                  {
                    "link": {
                      "#array": [{
                        "em": {
                          "@style": "bold",
                          "#text": "a bold link "
                        }
                      }],
                      "@target": "new",
                      "@href": "http://www.google.com"
                    }
                  },
                  {
                    "cite": {
                      "#text": "cite ",
                      "@entry": "A citation"
                    }
                  },
                  {
                    "term": {
                      "#text": "term "
                    }
                  },
                  {
                    "sub": {
                      "#text": "subscript "
                    }
                  },
                  {
                    "bdo": {
                      "#text": "this is bdo "
                    }
                  },
                  {
                    "sup": {
                      "#text": "superscript "
                    }
                  },
                  {
                    "var": {
                      "#text": "and use code "
                    }
                  },
                  {
                    "em": {
                      "@style": "bold",
                      "#text": "have seen bold "
                    }
                  },
                  {
                    "em": {
                      "@style": "bold",
                      "em": {
                        "@style": "italic",
                        "#text": "both bold and italic "
                      }
                    }
                  },
                  {
                    "em": {
                      "@style": "italic",
                      "#text": "italics"
                    }
                  },
                  {
                    "#text": ", for example a pair of equal and ."
                  }
                ]
                }
              },
              {
                "p": {
                  "#text": "Within this Character Data block"
                }
              },
              {
                "ul": {
                  "#array": [{
                    "li": {
                      "#text": "Unordered List item 1"
                    }},
                    {
                      "li": {
                      "#text": "Unordered List item 2"
                      }
                    }
                  ]
                }
              },
              {
                "ol": {
                  "#array": [{
                    "li": {
                      "#text": "Ordered List item 1"
                    }},
                    {
                      "li": {
                      "#text": "Ordered List item 2"
                      }
                    }
                  ]
                }
              },
              {
                "wb:inline": {
                  "@idref": "prose_light_side",
                  "@purpose": "learnbydoing"
                }
              }
            ]
          }
        }
      ]
    }
  }
 }