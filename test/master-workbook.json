 {
  "body": {
    "section": {
      "#array": [
        {
          "title": {
            "#text": "Section title"
          }
        },
        {
          "body": {
            "#array": [
              {
                "p": {
                  "#array": [
                  {
                    "#text": "As we "
                  },
                  {
                    "cite": {
                      "#text": "cite "
                    }
                  },
                  {
                    "term": {
                      "#text": "term "
                    }
                  },
                  {
                    "sub": {
                      "#text": "subscript "
                    }
                  },
                  {
                    "bdo": {
                      "#text": "this is bdo "
                    }
                  },
                  {
                    "sup": {
                      "#text": "superscript "
                    }
                  },
                  {
                    "var": {
                      "#text": "and use code "
                    }
                  },
                  {
                    "em": {
                      "@style": "bold",
                      "#text": "have seen bold "
                    }
                  },
                  {
                    "em": {
                      "@style": "italic",
                      "#text": "italics"
                    }
                  },
                  {
                    "#text": ", for example a pair of equal and ."
                  }
                ]
                }
              },
              {
                "p": {
