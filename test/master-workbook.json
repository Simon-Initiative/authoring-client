                }
              },
              {
                "ul": {
                  "#array": [{
                    "li": {
                      "#text": "Unordered List item 1"
                    }},
                    {
                      "li": {
                      "#text": "Unordered List item 2"
                      }
                    }
                  ]
                }
              },
              {
                "ol": {
                  "#array": [{
                    "li": {
                      "#text": "Ordered List item 1"
                    }},
                    {
                      "li": {
                      "#text": "Ordered List item 2"
                      }
                    }
                  ]
                }
              },
              {
                "wb:inline": {
                  "@idref": "prose_light_side",
                  "@purpose": "learnbydoing"
                }
              }
            ]
          }
        }
      ]
    }
  }
 }